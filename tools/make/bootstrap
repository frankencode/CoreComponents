#!/bin/sh -ex
SOURCE=$1
MACHINE=$(gcc -dumpmachine)
mkdir -p .modules-fluxkit_core_src-0.2.6-$MACHINE
mkdir -p .modules-fluxkit_core_tools-0.2.6-$MACHINE
mkdir -p .modules-fluxkit_tools_make-0.2.6-$MACHINE
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/AvlBalance.o $SOURCE/core/src/AvlBalance.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/BinaryTree.o $SOURCE/core/src/BinaryTree.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Arguments.o $SOURCE/core/src/Arguments.cpp
