gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Memory.o $SOURCE/core/src/Memory.cpp
