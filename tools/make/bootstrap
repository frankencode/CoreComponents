gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/RecipeProtocol.o $SOURCE/tools/make/RecipeProtocol.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/SystemPrerequisite.o $SOURCE/tools/make/SystemPrerequisite.cpp
