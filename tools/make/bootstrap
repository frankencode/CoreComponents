gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/UninstallStage.o $SOURCE/tools/make/UninstallStage.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/main.o $SOURCE/tools/make/main.cpp
gcc -pthread -o fluxmake -L. -Wl,--enable-new-dtags,-rpath=$PWD .modules-fluxkit_tools_make-$MACHINE/AnalyseStage.o .modules-fluxkit_tools_make-$MACHINE/BuildParameters.o .modules-fluxkit_tools_make-$MACHINE/BuildShell.o .modules-fluxkit_tools_make-$MACHINE/BuildPlan.o .modules-fluxkit_tools_make-$MACHINE/BuildStage.o .modules-fluxkit_tools_make-$MACHINE/CleanStage.o .modules-fluxkit_tools_make-$MACHINE/CompileLinkStage.o .modules-fluxkit_tools_make-$MACHINE/ConfigureStage.o .modules-fluxkit_tools_make-$MACHINE/DependencyCache.o .modules-fluxkit_tools_make-$MACHINE/GnuToolChain.o .modules-fluxkit_tools_make-$MACHINE/InstallStage.o .modules-fluxkit_tools_make-$MACHINE/JobScheduler.o .modules-fluxkit_tools_make-$MACHINE/JobServer.o .modules-fluxkit_tools_make-$MACHINE/RecipeProtocol.o .modules-fluxkit_tools_make-$MACHINE/SystemPrerequisite.o .modules-fluxkit_tools_make-$MACHINE/TestRunStage.o .modules-fluxkit_tools_make-$MACHINE/UninstallStage.o .modules-fluxkit_tools_make-$MACHINE/main.o -lfluxcore -lstdc++
