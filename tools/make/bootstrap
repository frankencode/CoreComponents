gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/ThreadFactory.o $SOURCE/core/flux/ThreadFactory.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Token.o $SOURCE/core/flux/Token.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/TimeoutLimiter.o $SOURCE/core/flux/TimeoutLimiter.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/TransferLimiter.o $SOURCE/core/flux/TransferLimiter.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/TransferMeter.o $SOURCE/core/flux/TransferMeter.cpp
