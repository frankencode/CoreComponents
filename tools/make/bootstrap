gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Group.o $SOURCE/core/src/Group.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/LineSource.o $SOURCE/core/src/LineSource.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/IntegerSyntax.o $SOURCE/core/src/IntegerSyntax.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/IoMonitor.o $SOURCE/core/src/IoMonitor.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/FormatSyntax.o $SOURCE/core/src/FormatSyntax.cpp
