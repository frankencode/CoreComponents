gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Format.o $SOURCE/core/src/Format.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/GlobalCoreMutex.o $SOURCE/core/src/GlobalCoreMutex.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/File.o $SOURCE/core/src/File.cpp
