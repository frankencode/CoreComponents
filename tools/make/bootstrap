#!/bin/sh -ex
SOURCE=$1
MACHINE=$(gcc -dumpmachine)
mkdir -p .modules-fluxkit_core_flux-0.1.0-$MACHINE
mkdir -p .modules-fluxkit_tools_make-$MACHINE
