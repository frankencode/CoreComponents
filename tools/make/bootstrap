gcc -shared -pthread -Wl,-soname,libfluxcore.so.0 -o libfluxcore.so.0.2.6 .modules-fluxkit_core_src-0.2.6-$MACHINE/AvlBalance.o .modules-fluxkit_core_src-0.2.6-$MACHINE/BinaryTree.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Arguments.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Condition.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Date.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ColorNames.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Color.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Dir.o .modules-fluxkit_core_src-0.2.6-$MACHINE/DebugToken.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Crc32.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ByteArray.o .modules-fluxkit_core_src-0.2.6-$MACHINE/DirWalker.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Event.o .modules-fluxkit_core_src-0.2.6-$MACHINE/FileLock.o .modules-fluxkit_core_src-0.2.6-$MACHINE/FloatSyntax.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Format.o .modules-fluxkit_core_src-0.2.6-$MACHINE/GlobalCoreMutex.o .modules-fluxkit_core_src-0.2.6-$MACHINE/File.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ExitEvent.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Glob.o .modules-fluxkit_core_src-0.2.6-$MACHINE/FileStatus.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Group.o .modules-fluxkit_core_src-0.2.6-$MACHINE/LineSource.o .modules-fluxkit_core_src-0.2.6-$MACHINE/IntegerSyntax.o .modules-fluxkit_core_src-0.2.6-$MACHINE/IoMonitor.o .modules-fluxkit_core_src-0.2.6-$MACHINE/FormatSyntax.o .modules-fluxkit_core_src-0.2.6-$MACHINE/List.o .modules-fluxkit_core_src-0.2.6-$MACHINE/KernelInfo.o .modules-fluxkit_core_src-0.2.6-$MACHINE/JsonWriter.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Map.o .modules-fluxkit_core_src-0.2.6-$MACHINE/MetaObject.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Pattern.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Mutex.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Memory.o .modules-fluxkit_core_src-0.2.6-$MACHINE/NullStream.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ResourceGuard.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Process.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Random.o .modules-fluxkit_core_src-0.2.6-$MACHINE/PatternSyntax.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ResourceContext.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SignalHandler.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ProcessFactory.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SignalSet.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Semaphore.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SignalMaster.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Stream.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SocketPair.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SpinLock.o .modules-fluxkit_core_src-0.2.6-$MACHINE/String.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SyntaxDebugger.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SyntaxDefinition.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SyntaxDebugFactory.o .modules-fluxkit_core_src-0.2.6-$MACHINE/System.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SystemStream.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Thread.o .modules-fluxkit_core_src-0.2.6-$MACHINE/SyntaxState.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Token.o .modules-fluxkit_core_src-0.2.6-$MACHINE/User.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Unicode.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ThreadExitEvent.o .modules-fluxkit_core_src-0.2.6-$MACHINE/ThreadFactory.o .modules-fluxkit_core_src-0.2.6-$MACHINE/YasonWriter.o .modules-fluxkit_core_src-0.2.6-$MACHINE/Variant.o .modules-fluxkit_core_src-0.2.6-$MACHINE/check.o .modules-fluxkit_core_src-0.2.6-$MACHINE/assert.o .modules-fluxkit_core_src-0.2.6-$MACHINE/exceptions.o .modules-fluxkit_core_src-0.2.6-$MACHINE/str.o .modules-fluxkit_core_src-0.2.6-$MACHINE/YasonSyntax.o .modules-fluxkit_core_src-0.2.6-$MACHINE/stdio.o .modules-fluxkit_core_src-0.2.6-$MACHINE/new.o .modules-fluxkit_core_src-0.2.6-$MACHINE/strings.o .modules-fluxkit_core_src-0.2.6-$MACHINE/yason.o .modules-fluxkit_core_src-0.2.6-$MACHINE/syntax.o -lstdc++
