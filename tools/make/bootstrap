gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/ResourceGuard.o $SOURCE/core/src/ResourceGuard.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Process.o $SOURCE/core/src/Process.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Random.o $SOURCE/core/src/Random.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/PatternSyntax.o $SOURCE/core/src/PatternSyntax.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/ResourceContext.o $SOURCE/core/src/ResourceContext.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SignalHandler.o $SOURCE/core/src/SignalHandler.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/ProcessFactory.o $SOURCE/core/src/ProcessFactory.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SignalSet.o $SOURCE/core/src/SignalSet.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Semaphore.o $SOURCE/core/src/Semaphore.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SignalMaster.o $SOURCE/core/src/SignalMaster.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/Stream.o $SOURCE/core/src/Stream.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SocketPair.o $SOURCE/core/src/SocketPair.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SpinLock.o $SOURCE/core/src/SpinLock.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/String.o $SOURCE/core/src/String.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SyntaxDebugger.o $SOURCE/core/src/SyntaxDebugger.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SyntaxDefinition.o $SOURCE/core/src/SyntaxDefinition.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/SyntaxDebugFactory.o $SOURCE/core/src/SyntaxDebugFactory.cpp
gcc -fPIC -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_core_src-0.2.6-$MACHINE/System.o $SOURCE/core/src/System.cpp
