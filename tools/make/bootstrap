rm -f libfluxcore.so.0
rm -f libfluxcore.so
ln -sf libfluxcore.so.0.1.0 libfluxcore.so.0.1
ln -sf libfluxcore.so.0.1.0 libfluxcore.so.0
ln -sf libfluxcore.so.0.1.0 libfluxcore.so
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/AnalyseStage.o $SOURCE/tools/make/AnalyseStage.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/BuildParameters.o $SOURCE/tools/make/BuildParameters.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/BuildShell.o $SOURCE/tools/make/BuildShell.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/BuildPlan.o $SOURCE/tools/make/BuildPlan.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/BuildStage.o $SOURCE/tools/make/BuildStage.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/CleanStage.o $SOURCE/tools/make/CleanStage.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/CompileLinkStage.o $SOURCE/tools/make/CompileLinkStage.cpp
gcc -fPIE -Wall -pthread -pipe -I$SOURCE/core -c -o .modules-fluxkit_tools_make-$MACHINE/ConfigureStage.o $SOURCE/tools/make/ConfigureStage.cpp
