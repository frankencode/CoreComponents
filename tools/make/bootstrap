rm -f libfluxcore.so.0
rm -f libfluxcore.so
ln -sf libfluxcore.so.0.2.6 libfluxcore.so.0.2
ln -sf libfluxcore.so.0.2.6 libfluxcore.so.0
ln -sf libfluxcore.so.0.2.6 libfluxcore.so
gcc -fPIE -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_tools_make-0.2.6-$MACHINE/AnalyseStage.o $SOURCE/tools/make/AnalyseStage.cpp
gcc -fPIE -Wall -pthread -pipe -DFLUXMAKE_BUNDLE_VERSION=0.2.6 -I$SOURCE/core/src/include -c -o .modules-fluxkit_tools_make-0.2.6-$MACHINE/BuildMap.o $SOURCE/tools/make/BuildMap.cpp
