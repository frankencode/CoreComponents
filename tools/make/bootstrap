gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/new.o $SOURCE/core/flux/new.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/stdio.o $SOURCE/core/flux/stdio.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/strings.o $SOURCE/core/flux/strings.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/str.o $SOURCE/core/flux/str.cpp
gcc -fPIC -Wall -pthread -pipe -c -o .modules-fluxkit_core_flux-0.1.0-$MACHINE/syntax.o $SOURCE/core/flux/syntax.cpp
gcc -shared -pthread -Wl,-soname,libfluxcore.so.0 -o libfluxcore.so.0.1.0 .modules-fluxkit_core_flux-0.1.0-$MACHINE/Base64.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/AbnfSyntaxDefinition.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Condition.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/ByteArray.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Config.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Crc32.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Date.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Dir.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/DirWalker.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Event.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Exception.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/ExitEvent.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/FileLock.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/File.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/FileStatus.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/FloatLiteral.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/FormatSpecifier.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Format.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/GlobalCoreMutex.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Glob.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Group.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/InetAddressSyntax.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/IntegerLiteral.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/IoMonitor.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/JsonWriter.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/LineBuffer.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/LineSource.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/LookAheadStream.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Mutex.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Memory.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/NullStream.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/NetworkInterface.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Pattern.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/PatternExpression.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/ProcessFactory.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Process.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Random.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/ProcessStatus.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Semaphore.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SignalSet.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SocketAddress.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SocketAddressEntry.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SpinLock.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Stream.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/StreamSocket.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/StreamTap.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/String.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SyntaxDebugFactory.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SyntaxDefinition.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SyntaxDebugger.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/System.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SyntaxState.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/SystemStream.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Thread.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/ThreadExitEvent.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/ThreadFactory.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Token.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/TimeoutLimiter.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/TransferLimiter.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/TransferMeter.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Unicode.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Uri.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/UriSyntax.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/User.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Error.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Variant.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/Yason.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/YasonSyntax.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/check.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/YasonWriter.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/new.o .modules-fluxkit_core_flux-0.1.0-$MACHINE/stdio.o .mod