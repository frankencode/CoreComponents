gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/strings.o $SOURCE/core/src/strings.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/str.o $SOURCE/core/src/str.cc
