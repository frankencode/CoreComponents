gcc -shared -pthread -Wl,-soname,libccsyntax.so.0 -o libccsyntax.so.0.8.1 .modules-cc_syntax_src-$MACHINE/FloatSyntax.o .modules-cc_syntax_src-$MACHINE/DebugToken.o .modules-cc_syntax_src-$MACHINE/SyntaxDefinition.o .modules-cc_syntax_src-$MACHINE/IntegerSyntax.o .modules-cc_syntax_src-$MACHINE/SyntaxState.o .modules-cc_syntax_src-$MACHINE/SyntaxDebugger.o .modules-cc_syntax_src-$MACHINE/SyntaxDebugFactory.o .modules-cc_syntax_src-$MACHINE/Token.o .modules-cc_syntax_src-$MACHINE/exceptions.o .modules-cc_syntax_src-$MACHINE/syntax.o -Wl,--no-as-needed -L. -lcccore -lstdc++ -Wl,--enable-new-dtags,-rpath='$ORIGIN',-rpath='$ORIGIN'/../lib,-rpath-link=$PWD
rm -f libccsyntax.so.0.8
rm -f libccsyntax.so.0
rm -f libccsyntax.so
ln -sf libccsyntax.so.0.8.1 libccsyntax.so.0.8
ln -sf libccsyntax.so.0.8.1 libccsyntax.so.0
ln -sf libccsyntax.so.0.8.1 libccsyntax.so
