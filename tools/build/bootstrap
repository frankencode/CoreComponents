#!/bin/sh -ex
SOURCE=$1
MACHINE=$(gcc -dumpmachine)
mkdir -p .modules-cc_core_src-$MACHINE
mkdir -p .modules-cc_core_tools-$MACHINE
mkdir -p .modules-cc_syntax_src-$MACHINE
mkdir -p .modules-cc_regexp_src-$MACHINE
mkdir -p .modules-cc_meta_src-$MACHINE
mkdir -p .modules-cc_tools_build-$MACHINE
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/BinaryTree.o $SOURCE/core/src/BinaryTree.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/AvlBalance.o $SOURCE/core/src/AvlBalance.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Arguments.o $SOURCE/core/src/Arguments.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/CleanupGuard.o $SOURCE/core/src/CleanupGuard.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/ColorNames.o $SOURCE/core/src/ColorNames.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Color.o $SOURCE/core/src/Color.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Crc32.o $SOURCE/core/src/Crc32.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Date.o $SOURCE/core/src/Date.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/CaptureSink.o $SOURCE/core/src/CaptureSink.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Bundle.o $SOURCE/core/src/Bundle.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/ByteArray.o $SOURCE/core/src/ByteArray.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Exception.o $SOURCE/core/src/Exception.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Crc16.o $SOURCE/core/src/Crc16.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Dir.o $SOURCE/core/src/Dir.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Format.o $SOURCE/core/src/Format.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/FileLock.o $SOURCE/core/src/FileLock.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/GlobalCoreMutex.o $SOURCE/core/src/GlobalCoreMutex.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/DirWalker.o $SOURCE/core/src/DirWalker.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/FileStatus.o $SOURCE/core/src/FileStatus.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/File.o $SOURCE/core/src/File.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/IoMonitor.o $SOURCE/core/src/IoMonitor.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/KernelInfo.o $SOURCE/core/src/KernelInfo.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/HexDump.o $SOURCE/core/src/HexDump.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Group.o $SOURCE/core/src/Group.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/LineSource.o $SOURCE/core/src/LineSource.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Map.o $SOURCE/core/src/Map.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Random.o $SOURCE/core/src/Random.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/MemoryAccounting.o $SOURCE/core/src/MemoryAccounting.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/NullStream.o $SOURCE/core/src/NullStream.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/MemoryStream.o $SOURCE/core/src/MemoryStream.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/RandomSource.o $SOURCE/core/src/RandomSource.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Memory.o $SOURCE/core/src/Memory.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/ResourceContext.o $SOURCE/core/src/ResourceContext.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/ResourceGuard.o $SOURCE/core/src/ResourceGuard.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/ReplaySource.o $SOURCE/core/src/ReplaySource.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Mutex.o $SOURCE/core/src/Mutex.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Semaphore.o $SOURCE/core/src/Semaphore.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/SignalSet.o $SOURCE/core/src/SignalSet.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/SignalMaster.o $SOURCE/core/src/SignalMaster.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Process.o $SOURCE/core/src/Process.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/SpinLock.o $SOURCE/core/src/SpinLock.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Stream.o $SOURCE/core/src/Stream.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/SystemIo.o $SOURCE/core/src/SystemIo.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/System.o $SOURCE/core/src/System.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/SubProcess.o $SOURCE/core/src/SubProcess.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Unicode.o $SOURCE/core/src/Unicode.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Thread.o $SOURCE/core/src/Thread.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/SystemStream.o $SOURCE/core/src/SystemStream.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/User.o $SOURCE/core/src/User.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Variant.o $SOURCE/core/src/Variant.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/check.o $SOURCE/core/src/check.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/UnlinkGuard.o $SOURCE/core/src/UnlinkGuard.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/Version.o $SOURCE/core/src/Version.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/assert.o $SOURCE/core/src/assert.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/WaitCondition.o $SOURCE/core/src/WaitCondition.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/new.o $SOURCE/core/src/new.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/strings.o $SOURCE/core/src/strings.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/exceptions.o $SOURCE/core/src/exceptions.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/str.o $SOURCE/core/src/str.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-$MACHINE/stdio.o $SOURCE/core/src/stdio.cc &
wait
gcc -shared -pthread -Wl,-soname,libcccore.so.0 -o libcccore.so.0.8.1 .modules-cc_core_src-$MACHINE/BinaryTree.o .modules-cc_core_src-$MACHINE/AvlBalance.o .modules-cc_core_src-$MACHINE/Arguments.o .modules-cc_core_src-$MACHINE/CleanupGuard.o .modules-cc_core_src-$MACHINE/ColorNames.o .modules-cc_core_src-$MACHINE/Color.o .modules-cc_core_src-$MACHINE/Crc32.o .modules-cc_core_src-$MACHINE/Date.o .modules-cc_core_src-$MACHINE/CaptureSink.o .modules-cc_core_src-$MACHINE/Bundle.o .modules-cc_core_src-$MACHINE/ByteArray.o .modules-cc_core_src-$MACHINE/Exception.o .modules-cc_core_src-$MACHINE/Crc16.o .modules-cc_core_src-$MACHINE/Dir.o .modules-cc_core_src-$MACHINE/Format.o .modules-cc_core_src-$MACHINE/FileLock.o .modules-cc_core_src-$MACHINE/GlobalCoreMutex.o .modules-cc_core_src-$MACHINE/DirWalker.o .modules-cc_core_src-$MACHINE/FileStatus.o .modules-cc_core_src-$MACHINE/File.o .modules-cc_core_src-$MACHINE/IoMonitor.o .modules-cc_core_src-$MACHINE/KernelInfo.o .modules-cc_core_src-$MACHINE/HexDump.o .modules-cc_core_src-$MACHINE/Group.o .modules-cc_core_src-$MACHINE/LineSource.o .modules-cc_core_src-$MACHINE/Map.o .modules-cc_core_src-$MACHINE/Random.o .modules-cc_core_src-$MACHINE/MemoryAccounting.o .modules-cc_core_src-$MACHINE/NullStream.o .modules-cc_core_src-$MACHINE/MemoryStream.o .modules-cc_core_src-$MACHINE/RandomSource.o .modules-cc_core_src-$MACHINE/Memory.o .modules-cc_core_src-$MACHINE/ResourceContext.o .modules-cc_core_src-$MACHINE/ResourceGuard.o .modules-cc_core_src-$MACHINE/ReplaySource.o .modules-cc_core_src-$MACHINE/Mutex.o .modules-cc_core_src-$MACHINE/Semaphore.o .modules-cc_core_src-$MACHINE/SignalSet.o .modules-cc_core_src-$MACHINE/SignalMaster.o .modules-cc_core_src-$MACHINE/Process.o .modules-cc_core_src-$MACHINE/SpinLock.o .modules-cc_core_src-$MACHINE/Stream.o .modules-cc_core_src-$MACHINE/SystemIo.o .modules-cc_core_src-$MACHINE/System.o .modules-cc_core_src-$MACHINE/SubProcess.o .modules-cc_core_src-$MACHINE/Unicode.o .modules-cc_core_src-$MACHINE/Thread.o .modules-cc_core_src-$MACHINE/SystemStream.o .modules-cc_core_src-$MACHINE/User.o .modules-cc_core_src-$MACHINE/Variant.o .modules-cc_core_src-$MACHINE/check.o .modules-cc_core_src-$MACHINE/UnlinkGuard.o .modules-cc_core_src-$MACHINE/Version.o .modules-cc_core_src-$MACHINE/assert.o .modules-cc_core_src-$MACHINE/WaitCondition.o .modules-cc_core_src-$MACHINE/new.o .modules-cc_core_src-$MACHINE/strings.o .modules-cc_core_src-$MACHINE/exceptions.o .modules-cc_core_src-$MACHINE/str.o .modules-cc_core_src-$MACHINE/stdio.o -Wl,--no-as-needed -L. -lstdc++ -Wl,--enable-new-dtags,-rpath='$ORIGIN',-rpath='$ORIGIN'/../lib,-rpath-link=$PWD
rm -f libcccore.so.0.8
rm -f libcccore.so.0
rm -f libcccore.so
ln -sf libcccore.so.0.8.1 libcccore.so.0.8
ln -sf libcccore.so.0.8.1 libcccore.so.0
ln -sf libcccore.so.0.8.1 libcccore.so
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/FloatSyntax.o $SOURCE/syntax/src/FloatSyntax.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/DebugToken.o $SOURCE/syntax/src/DebugToken.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/SyntaxDefinition.o $SOURCE/syntax/src/SyntaxDefinition.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/IntegerSyntax.o $SOURCE/syntax/src/IntegerSyntax.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/SyntaxState.o $SOURCE/syntax/src/SyntaxState.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/SyntaxDebugger.o $SOURCE/syntax/src/SyntaxDebugger.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/SyntaxDebugFactory.o $SOURCE/syntax/src/SyntaxDebugFactory.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/Token.o $SOURCE/syntax/src/Token.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/exceptions.o $SOURCE/syntax/src/exceptions.cc &
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/syntax/src/include -I$SOURCE/core/src/include -c -o .modules-cc_syntax_src-$MACHINE/syntax.o $SOURCE/syntax/src/syntax.cc &
