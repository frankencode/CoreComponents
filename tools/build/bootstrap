gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.8.1 -std=c++11 -I$SOURCE/regexp/src/include -I$SOURCE/core/src/include -I$SOURCE/syntax/src/include -c -o .modules-cc_regexp_src-$MACHINE/RegExp.o $SOURCE/regexp/src/RegExp.cc &
