gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/BinaryTree.o $SOURCE/core/src/BinaryTree.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Color.o $SOURCE/core/src/Color.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Crc32.o $SOURCE/core/src/Crc32.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/CaptureSink.o $SOURCE/core/src/CaptureSink.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Clock.o $SOURCE/core/src/Clock.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Crc16.o $SOURCE/core/src/Crc16.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Date.o $SOURCE/core/src/Date.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/GlobalCoreMutex.o $SOURCE/core/src/GlobalCoreMutex.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Dir.o $SOURCE/core/src/Dir.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Format.o $SOURCE/core/src/Format.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/FileStatus.o $SOURCE/core/src/FileStatus.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/FileLock.o $SOURCE/core/src/FileLock.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/File.o $SOURCE/core/src/File.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/HexDump.o $SOURCE/core/src/HexDump.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/IoMonitor.o $SOURCE/core/src/IoMonitor.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/DirWalker.o $SOURCE/core/src/DirWalker.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/LineSource.o $SOURCE/core/src/LineSource.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Group.o $SOURCE/core/src/Group.cc
