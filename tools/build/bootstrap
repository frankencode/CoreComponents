gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Map.o $SOURCE/core/src/Map.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/NullStream.o $SOURCE/core/src/NullStream.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Mutex.o $SOURCE/core/src/Mutex.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/MemoryStream.o $SOURCE/core/src/MemoryStream.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Memory.o $SOURCE/core/src/Memory.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/RandomSource.o $SOURCE/core/src/RandomSource.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Random.o $SOURCE/core/src/Random.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ReplaySource.o $SOURCE/core/src/ReplaySource.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ResourceContext.o $SOURCE/core/src/ResourceContext.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Semaphore.o $SOURCE/core/src/Semaphore.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ResourceGuard.o $SOURCE/core/src/ResourceGuard.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SpinLock.o $SOURCE/core/src/SpinLock.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SignalSet.o $SOURCE/core/src/SignalSet.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SignalMaster.o $SOURCE/core/src/SignalMaster.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Process.o $SOURCE/core/src/Process.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Stream.o $SOURCE/core/src/Stream.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/System.o $SOURCE/core/src/System.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SystemIo.o $SOURCE/core/src/SystemIo.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Unicode.o $SOURCE/core/src/Unicode.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Thread.o $SOURCE/core/src/Thread.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SubProcess.o $SOURCE/core/src/SubProcess.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SystemStream.o $SOURCE/core/src/SystemStream.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/User.o $SOURCE/core/src/User.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Variant.o $SOURCE/core/src/Variant.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/UnlinkGuard.o $SOURCE/core/src/UnlinkGuard.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Version.o $SOURCE/core/src/Version.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/check.o $SOURCE/core/src/check.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/assert.o $SOURCE/core/src/assert.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/exceptions.o $SOURCE/core/src/exceptions.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/WaitCondition.o $SOURCE/core/src/WaitCondition.cc
