#!/bin/sh -ex
SOURCE=$1
MACHINE=$(gcc -dumpmachine)
mkdir -p .modules-cc_core_src-0.7.2-release-O2-$MACHINE
mkdir -p .modules-cc_core_tools-0.7.2-release-O2-$MACHINE
mkdir -p .modules-cc_syntax_src-0.7.2-release-O2-$MACHINE
mkdir -p .modules-cc_regexp_src-0.7.2-release-O2-$MACHINE
mkdir -p .modules-cc_meta_src-0.7.2-release-O2-$MACHINE
mkdir -p .modules-cc_tools_build-0.7.2-release-O2-$MACHINE
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/AvlBalance.o $SOURCE/core/src/AvlBalance.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Arguments.o $SOURCE/core/src/Arguments.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/CleanupGuard.o $SOURCE/core/src/CleanupGuard.cc
gcc -DNDEBUG -O2 -fPIC -Wall -pthread -pipe -DCCBUILD_BUNDLE_VERSION=0.7.2 -std=c++11 -I$SOURCE/core/src/include -c -o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ColorNames.o $SOURCE/core/src/ColorNames.cc
