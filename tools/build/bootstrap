gcc -shared -pthread -Wl,-soname,libcccore.so.0 -o libcccore.so.0.7.2 .modules-cc_core_src-0.7.2-release-O2-$MACHINE/AvlBalance.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Arguments.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/CleanupGuard.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ColorNames.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ByteArray.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/BinaryTree.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Color.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Crc32.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/CaptureSink.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Clock.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Crc16.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Date.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/GlobalCoreMutex.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Dir.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Format.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/FileStatus.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/FileLock.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/File.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/HexDump.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/IoMonitor.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/DirWalker.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/LineSource.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Group.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/KernelInfo.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Map.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/NullStream.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Mutex.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/MemoryStream.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Memory.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/RandomSource.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Random.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ReplaySource.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ResourceContext.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Semaphore.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/ResourceGuard.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SpinLock.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SignalSet.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SignalMaster.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Process.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Stream.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/System.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SystemIo.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Unicode.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Thread.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SubProcess.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/SystemStream.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/User.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Variant.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/UnlinkGuard.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/Version.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/check.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/assert.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/exceptions.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/WaitCondition.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/new.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/strings.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/str.o .modules-cc_core_src-0.7.2-release-O2-$MACHINE/stdio.o -Wl,--no-as-needed -L. -lstdc++ -Wl,--enable-new-dtags,-rpath='$ORIGIN',-rpath='$ORIGIN'/../lib,-rpath-link=$PWD
