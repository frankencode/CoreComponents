#pragma once

#include <cc/buckets/config>

namespace cc {

/** \internal
  * \brief Storage policy bucket tree based container
  * \tparam T Item type
  */
template<class T>
struct BucketStoragePolicy
{
    static constexpr unsigned Granularity = BucketTreeGranularity;
};

} // namespace cc
