#include <cc/meta/MetaPrototype>

namespace cc {
namespace toki {

class Palette;

class PalettePrototype: public meta::MetaPrototype
{
public:
    static Ref<PalettePrototype> create();

private:
    PalettePrototype();

    Ref<MetaObject> produce() const override;
};

}} // namespace cc::toki
