#pragma once

#include <cc/http/HttpRequest>
#include <cc/FileStatus>

namespace cc {
namespace http {

class ScriptHandler: public Object
{
public:
    virtual bool process(const HttpRequest *request, const FileStatus *status, const string &documentRoot = "") = 0;
};

}} // namespace cc::http
