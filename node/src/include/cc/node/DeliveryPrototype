#include <cc/meta/MetaPrototype>
#include <cc/meta/MetaProtocol>

namespace cc {
namespace node {

using meta::MetaPrototype;
using meta::MetaProtocol;

class DeliveryPrototype: public MetaPrototype
{
public:
    static Ref<DeliveryPrototype> create(const String &className);

protected:
    DeliveryPrototype(const String &className, const MetaProtocol *protocol = nullptr);
};

}} // namespace cc::node
