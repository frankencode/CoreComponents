#include <cc/meta/MetaPrototype>
#include <cc/node/DeliveryProtocol>

namespace cc {
namespace node {

class DeliveryPrototype: public meta::MetaPrototype
{
public:
    static Ref<DeliveryPrototype> create(const String &className);

protected:
    DeliveryPrototype(const String &className, const DeliveryProtocol *protocol = DeliveryProtocol::defaultInstance());
};

}} // namespace cc::node
