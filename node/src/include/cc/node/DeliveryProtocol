#include <cc/meta/MetaProtocol>

namespace cc { template<class> class Singleton; }

namespace cc {
namespace node {

using meta::MetaProtocol;

class DeliveryProtocol: public MetaProtocol
{
public:
    static const DeliveryProtocol *defaultInstance();

protected:
    friend class Singleton<DeliveryProtocol>;
    DeliveryProtocol();
};

}} // namespace cc::node
