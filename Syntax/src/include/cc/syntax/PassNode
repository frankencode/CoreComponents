#pragma once

#include <cc/syntax/SyntaxNode>

namespace cc::syntax {

/** \brief Unconditionally succeed to match at the current text position
  */
class PassNode final: public SyntaxNode
{
public:
    /** &nbsp;
      */
    PassNode();

private:
    struct State;
};

} // namespace cc::syntax
