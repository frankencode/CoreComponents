mkdir -p .modules-FTL_mach-release-optimize-speed-2-$MACHINE
g++ -DNDEBUG -O2 -fpic -Wall -pthread -c -o .modules-FTL_core_ftl-0.3.0-release-optimize-speed-2-$MACHINE/syntax.o $SOURCE/core/ftl/syntax.cpp
g++ -DNDEBUG -O2 -fpic -Wall -pthread -c -o .modules-FTL_core_ftl-0.3.0-release-optimize-speed-2-$MACHINE/ByteEncoder.o $SOURCE/core/ftl/ByteEncoder.cpp
