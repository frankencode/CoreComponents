#include <cc/HttpServiceInstance>

namespace cc {

class TimeWebSocketInstance final: public HttpServiceInstance
{
public:
    TimeWebSocketInstance() = default;
    TimeWebSocketInstance(const MetaObject &config);

private:
    friend class Object;

    struct State final: public HttpServiceInstance::State
    {
        State(const MetaObject &config);
        HttpServiceDelegate createDelegate() const override;
    };

    const State &me() const;
};

} // namespace cc
