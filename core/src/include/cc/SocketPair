#pragma once

#include <cc/IoChannel>

namespace cc {

/** \brief Bi-directional I/O channel
  * \see Spawn
  */
class SocketPair: public IoChannel
{
public:
    static Ref<SocketPair> create();

protected:
    SocketPair();
};

} // namespace cc
